//
#include <iostream>
#include <vector>
#include <thread>
#include <mutex>
#include <condition_variable>
#include <chrono>
#include <random>
#include <algorithm>
#include <ncurses.h>

using namespace std;

// Constants for the game
const int width = 40;
const int height = 20;
const int num_ghosts = 5;
const char player_char = '@';
const char ghost_char = 'G';
const char wall_char = '#';
const char empty_char = ' ';
const char food_char = '.';
const char power_up_char = '*';

// Game state variables
char map[height][width];
int player_x, player_y;
int score = 0;
bool game_over = false;
bool power_up_active = false;
int power_up_timer = 0;
std::vector<std::pair<int, int>> ghosts(num_ghosts);
std::mutex mtx; // Mutex for thread synchronization
std::condition_variable cv;
bool player_moved = false;

// Function to initialize the game map
void initialize_map() {
    for (int i = 0; i < height; ++i) {
        for (int j = 0; j < width; ++j) {
            if (i == 0 || i == height - 1 || j == 0 || j == width - 1 || (rand() % 5 == 0)) {
                map[i][j] = wall_char;
            } else {
                map[i][j] = food_char;
            }
        }
    }
    player_x = width / 2;
    player_y = height / 2;
    map[player_y][player_x] = player_char;

    // Place power-ups randomly
    for (int i = 0; i < 3; ++i) {
        int px, py;
        do {
            px = rand() % (width - 2) + 1;
            py = rand() % (height - 2) + 1;
        } while (map[py][px] != food_char);
        map[py][px] = power_up_char;
    }

    // Initialize ghost positions
    for (int i = 0; i < num_ghosts; ++i) {
        do {
            ghosts[i].first = rand() % (width - 2) + 1;
            ghosts[i].second = rand() % (height - 2) + 1;
        } while (map[ghosts[i].second][ghosts[i].first] != food_char);
        map[ghosts[i].second][ghosts[i].first] = ghost_char;
    }
}

// Function to display the game map
void display_map() {
    clear(); // Clear the screen

    for (int i = 0; i < height; ++i) {
        for (int j = 0; j < width; ++j) {
            mvaddch(i, j, map[i][j]);
        }
    }

    mvprintw(height, 0, "Score: %d", score);
    if (power_up_active) {
        mvprintw(height + 1, 0, "Power-up active! Timer: %d", power_up_timer);
    }
    refresh(); // Refresh the screen to show changes
}

// Function to handle player input
void handle_input() {
    int ch = getch();
    std::unique_lock<std::mutex> lock(mtx);
    int new_x = player_x;
    int new_y = player_y;

    switch (ch) {
        case KEY_UP:    new_y--; break;
        case KEY_DOWN:  new_y++; break;
        case KEY_LEFT:  new_x--; break;
        case KEY_RIGHT: new_x++; break;
        case 'q': game_over = true; break; // Quit game
    }

    if (map[new_y][new_x] != wall_char) {
        if (map[new_y][new_x] == food_char) {
            score++;
        } else if (map[new_y][new_x] == power_up_char) {
            power_up_active = true;
            power_up_timer = 20; // Power-up duration (20 moves)
            score += 5; // Bonus points for power-up
        }
        map[player_y][player_x] = empty_char;
        player_x = new_x;
        player_y = new_y;
        map[player_y][player_x] = player_char;
        player_moved = true;
    }
    lock.unlock();
    cv.notify_one(); // Notify ghost thread that player has moved
}

// Function to update ghost positions
void update_ghosts() {
    while (!game_over) {
        std::unique_lock<std::mutex> lock(mtx);
        // Wait until player has moved or game is over
        cv.wait(lock, []{ return player_moved || game_over; });
        if (game_over) break;

        for (int i = 0; i < num_ghosts; ++i) {
            if (map[ghosts[i].second][ghosts[i].first] == ghost_char) {
                map[ghosts[i].second][ghosts[i].first] = food_char; // Restore food pellet
            } else {
                map[ghosts[i].second][ghosts[i].first] = empty_char; // Or empty if it was power-up
            }


            int dx = 0, dy = 0;
            if (player_x > ghosts[i].first) dx = 1;
            else if (player_x < ghosts[i].first) dx = -1;
            if (player_y > ghosts[i].second) dy = 1;
            else if (player_y < ghosts[i].second) dy = -1;

            int new_gx = ghosts[i].first + dx;
            int new_gy = ghosts[i].second + dy;

            // Basic wall collision for ghosts
            if (map[new_gy][ghosts[i].first] != wall_char) {
                ghosts[i].second = new_gy;
            } else if (map[ghosts[i].second][new_gx] != wall_char) {
                ghosts[i].first = new_gx;
            }
            // More sophisticated pathfinding could be added here

            map[ghosts[i].second][ghosts[i].first] = ghost_char;
        }

        // Check for collision with player
        for (int i = 0; i < num_ghosts; ++i) {
            if (player_x == ghosts[i].first && player_y == ghosts[i].second) {
                if (power_up_active) {
                    // Ghost is eaten
                    score += 10; // Bonus for eating a ghost
                    // Relocate ghost
                    do {
                        ghosts[i].first = rand() % (width - 2) + 1;
                        ghosts[i].second = rand() % (height - 2) + 1;
                    } while (map[ghosts[i].second][ghosts[i].first] != food_char);
                    map[ghosts[i].second][ghosts[i].first] = ghost_char;
                } else {
                    game_over = true; // Player is caught
                    break;
                }
            }
        }
        player_moved = false; // Reset player moved flag
        lock.unlock();
        std::this_thread::sleep_for(std::chrono::milliseconds(200)); // Ghost move speed
    }
}

// Function to update power-up timer
void update_power_up() {
    while (!game_over) {
        std::this_thread::sleep_for(std::chrono::seconds(1));
        std::unique_lock<std::mutex> lock(mtx);
        if (power_up_active) {
            power_up_timer--;
            if (power_up_timer <= 0) {
                power_up_active = false;
                log_message("Power-up expired.");
            }
        }
        lock.unlock();
    }
}

int main() {
    // Initialize ncurses
    initscr();
    cbreak();
    noecho();
    keypad(stdscr, TRUE);
    curs_set(0); // Hide cursor
    timeout(100); // Non-blocking getch

    srand(time(0)); // Seed random number generator

    initialize_map();

    // Start ghost thread
    std::thread ghost_thread(update_ghosts);
    std::thread power_up_thread(update_power_up);

    // Game loop
    while (!game_over) {
        display_map();
        handle_input();
        // Check for win condition (all food eaten)
        bool food_left = false;
        for (int i = 0; i < height; ++i) {
            for (int j = 0; j < width; ++j) {
                if (map[i][j] == food_char) {
                    food_left = true;
                    break;
                }
            }
            if (food_left) break;
        }
        if (!food_left) {
            game_over = true;
            log_message("You won! All food eaten.");
        }
    }

    // Game over screen
    clear();
    mvprintw(height / 2, width / 2 - 5, "GAME OVER");
    mvprintw(height / 2 + 1, width / 2 - 7, "Final Score: %d", score);
    refresh();
    std::this_thread::sleep_for(std::chrono::seconds(3));

    // Clean up ncurses
    endwin();

    // Join ghost thread
    cv.notify_all(); // Notify threads to exit if waiting
    if (ghost_thread.joinable()) {
        ghost_thread.join();
    }
    if (power_up_thread.joinable()) {
        power_up_thread.join();
    }

    return 0;
}

