    "commander": "^11.1.0",
    "dotenv": "^17.2.1",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^6.0.6",
    "js-yaml": "^4.1.0",
    "ora": "^8.2.0",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=18.0.0"
